{
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2019"
    ],
    "declaration": true,
    "declarationDir": "dts",
    "emitDeclarationOnly": true,
    "moduleResolution": "node",
    "target": "es2018",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
